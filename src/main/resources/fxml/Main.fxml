      <BorderPane fx:id="borderPane1" minHeight="-Infinity" minWidth="-Infinity" prefHeight="652.0" prefWidth="900.0">
         <top>
            <HBox alignment="TOP_RIGHT" styleClass="top-background" BorderPane.alignment="TOP_RIGHT">
               <children>
                  <ImageView fitWidth="24.0" onMouseClicked="#close" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/close.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets bottom="5.0" right="10.0" top="5.0" />
               </padding>
            </HBox>
         </top>
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="gral-background" BorderPane.alignment="CENTER">
               <children>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#archivoLoadScene" prefHeight="25.0" prefWidth="114.0" styleClass="boton-left-side-menu" text="Archivo" />
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#nodosLoadScene" prefHeight="50.0" prefWidth="114.0" styleClass="boton-left-side-menu" text="Nodos">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin></Button>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#elementosLoadScene" prefHeight="50.0" prefWidth="114.0" styleClass="boton-left-side-menu" text="Elementos">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin></Button>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#clear" prefHeight="50.0" prefWidth="114.0" styleClass="boton-left-side-menu" text="Clear" fx:id="clearButton">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Button>
               </children>
               <BorderPane.margin>
                  <Insets right="3.0" />
               </BorderPane.margin>
            </VBox>
         </left>
      </BorderPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
